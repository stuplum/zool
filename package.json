{
  "name": "zool",
  "version": "0.5.23",
  "description": "Living Styleguide Generator",
  "main": "app/index.js",
  "bin": {
    "zool": "bin.js"
  },
  "scripts": {
    "build": "webpack --progress --colors",
    "start": "npm run build && node ./index",
    "dev": "NODE_ENV=development npm start",
    "test": "karma start test/karma.conf.js --singleRun=true && npm run test-server",
    "test-client": "karma start test/karma.conf.js",
    "test-server": "lab -I 'describe,it,before,beforeEach,after,afterEach,expect,syncSpec,fromRoot,__core-js_shared__' -T test/support/transforms.js test/unit.lab.js"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/stuplum/zool"
  },
  "keywords": [
    "styleguide",
    "style",
    "guide"
  ],
  "author": "stuplum",
  "bugs": {
    "url": "https://github.com/stuplum/zool/issues"
  },
  "homepage": "https://github.com/stuplum/zool",
  "license": "ISC",
  "dependencies": {
    "boom": "3.1.2",
    "config": "^1.16.0",
    "decamelize": "1.2.0",
    "directory-tree": "0.1.1",
    "glue": "3.2.0",
    "handlebars": "^4.0.2",
    "hapi": "12.1.0",
    "highlight.js": "9.1.0",
    "hoek": "^2.16.3",
    "inert": "^3.2.0",
    "marked": "^0.3.5",
    "moment": "2.11.2",
    "nunjucks": "^2.2.0",
    "vision": "^4.0.1",
    "walk": "2.3.9",
    "yargs": "3.32.0",
    "zool-sass": "0.2.1",
    "zool-static-assets": "0.3.0",
    "zool-utils": "0.2.0",
    "zool-webpack": "0.2.0"
  },
  "devDependencies": {
    "babel-core": "6.7.6",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "code": "^1.5.0",
    "glob": "^5.0.15",
    "inject-loader": "2.0.1",
    "jasmine-core": "2.4.1",
    "jasmine-jquery-matchers": "1.1.1",
    "jquery": "2.2.3",
    "karma": "0.13.22",
    "karma-chrome-launcher": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-sinon": "1.0.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "lab": "^6.2.0",
    "promise-matchers": "0.9.6",
    "webpack": "1.12.15",
    "webpack-dev-server": "1.14.1",
    "zool-test-support": "0.1.0"
  }
}
